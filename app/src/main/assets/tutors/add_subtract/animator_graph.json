{
  "animatorMap": {
    "COMMENT": "Animation Graph for the Add/Subtract Tutor",

    "add_subtract": {

      "type": "ANIMATOR",
      "title": "Add/Subtract tutor",
      "COMMENT": "This tutor is designed for the Add/Subtract tutor.",
      "version": "1.0.0",
      "rootnode": "INTRO_STATE",

      "nodeMap": {

        "COMMENT": "@@@@@  CNodes  @@@@@",

        "INTRO_STATE": {
          "type": "NODE",
          "COMMENT": "Intro Clip",
          "maptype":"moduleMap",
          "mapname": "PLAYINTRO",
          "preenter":["SET_VERSION","SET_DATASOURCE"],
          "preexit" :[],
          "edges":
          [
            {"constraint":"","edge": "PROBLEM"}
          ]
        },

        // This initializes the dataset display and resets for a new set of attempts
        //
        "PROBLEM":{
          "COMMENT":"This node presents the first problem.",
          "type":"node",
          "maptype":"moduleMap",
          "mapname": "SHOWPROBLEM",
          "preenter":[],
          "preexit" :[],
          "edges":
          [
            {"constraint":"","edge":"NEXTDIGIT"}
          ]
        },

        "NEXTDIGIT":{
          "COMMENT":"This node presents the first digit.",
          "type":"node",
          "maptype":"moduleMap",
          "mapname": "SHOWDIGIT",
          "preenter":["SET_DOTBAGS_INVISIBLE"],
          "preexit" :["PROMPT_COL"],
          "edges":
          [
            {"constraint":"","edge":"USERINPUT"}
          ]
        },

        // This waits for input
        //
        "USERINPUT":{
          "COMMENT":"When the user touches for the first time",
          "type":"node",
          "maptype":"moduleMap",
          "mapname": "CLICKDOTS",
          "preenter":[],
          "preexit" :[],
          "edges":
          [
            {"constraint":"ASM_ADD","edge":"ADD_PROMPT"},
            {"constraint":"ASM_SUBTRACT","edge":"SUB_PROMPT"},
            {"constraint":"ASM_MULTI","edge":"MULTI_PROMPT"}
            //{"constraint":"", "edge":"REPEATED_ADD"}
          ]
        },

        "ADD_PROMPT":{
          "COMMENT":"When the user hesitates",
          "type":"node",
          "maptype":"moduleMap",
          "mapname": "PROMPT_IN_ADD",
          "preenter":[],
          "preexit" :[],
          "edges":
          [
            {"constraint":"CLICK_ON_DOT","edge":"ADD_PROMPT"},
            {"constraint":"FTR_RIGHT","edge":"WHOLECORRECT"},
            {"constraint":"FTR_WRONG","edge":"WHOLEWRONG"}
          ]
        },

        "SUB_PROMPT":{
          "COMMENT":"When the user hesitates",
          "type":"node",
          "maptype":"moduleMap",
          "mapname": "PROMPT_IN_SUB",
          "preenter":[],
          "preexit" :[],
          "edges":
          [
            {"constraint":"CLICK_ON_DOT","edge":"SUB_PROMPT"},
            {"constraint":"FTR_RIGHT","edge":"WHOLECORRECT"},
            {"constraint":"FTR_WRONG","edge":"WHOLEWRONG"}
          ]
        },

        "MULTI_PROMPT":{
          "COMMENT":"When the user hesitates",
          "type":"node",
          "maptype":"moduleMap",
          "mapname": "PROMPT_IN_MULTI",
          "preenter":[],
          "preexit" :[],
          "edges":
          [
            {"constraint":"FTR_RIGHT","edge":"WHOLECORRECT"},
            {"constraint":"FTR_WRONG","edge":"WHOLEWRONG"}
          ]
        },

        "WHOLECORRECT":{
          "COMMENT":"When user inputs a correct answer...",
          "type":"node",
          "maptype":"moduleMap",
          "mapname": "PLAYCORRECT",
          "preenter":[],
          "preexit" :[],
          "edges":
          [
            {"constraint":"COND_PASS1","edge":"EXITCLIP1"},
            {"constraint":"COND_PASS2","edge":"EXITCLIP2"},
            {"constraint":"","edge":"PROBLEM"}
          ]
        },

        "WHOLEWRONG":{
          "COMMENT":"When user inputs an incorrect answer...",
          "type":"node",
          "maptype":"moduleMap",
          "mapname": "EVALUATEDIGIT",
          "preenter":[],
          "preexit" :[],
          "edges":
          [
            {"constraint":"FTR_RIGHT","edge":"NEXTDIGIT"},
            {"constraint":"FTR_WRONG","edge":"USERINPUT"}
          ]
        },

        "EXITCLIP1": {
          "COMMENT":"Exit Clip for ALL_CORRECT",
          "type":"node",
          "maptype":"moduleMap",
          "mapname": "PLAYEXIT1",
          "preenter":[],
          "preexit" :[],
          "edges":
          [
            {"constraint":"","edge":"NEXTSCENE"}
          ]
        },

        "EXITCLIP2": {"COMMENT":"Exit Clip for NOT_ALL_CORRECT",
          "type":"node",
          "maptype":"moduleMap",
          "mapname": "PLAYEXIT2",
          "preenter":[],
          "preexit" :[],
          "edges":
          [
            {"constraint":"","edge":"NEXTSCENE"}
          ]
        },

        "NEXTSCENE": {
          "type": "NODE",
          "COMMENT": "When module is complete - move to next scene in the scenegraph",
          "maptype":"actionMap",
          "mapname":"GOTONEXTSCENE",
          "preenter": [],
          "preexit": [],
          "edges": [
          ]
        }
      },

