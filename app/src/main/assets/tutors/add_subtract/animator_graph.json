        },

        "NOT_ALL_CORRECT": {
          "type": "CONDITION",
          "test": "!ALL_CORRECT"
        },

        "FTR_RIGHT": {
          "type": "CONDITION",
          "test": "FTR_RIGHT"
        },

        "FTR_WRONG": {
          "type": "CONDITION",
