            {"name": "LOG_EVENT", "type": "QUEUEDCOMMAND", "id": "SBubblePop", "method": "logState", "parms": "type#node,value#EMPTYMODULE:String", "features":""}
          ]
        },

        "FINALSCORE": {
          "type": "MODULE",
          "reuse":true,
          "COMMENT": "????",
          "tracks": [
